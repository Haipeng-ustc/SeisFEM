void shape_t6(double r, double s, double t[], double dtdr[], double dtds[] )

/*
*****************************************************************************

 SHAPE_T10 evaluates shape functions for a 10 node reference triangle.

  Reference Element T10:

    |
    1  10
    |  |\
    |  | \
    |  8  9
    |  |   \
    S  |    \
    |  5  6  7
    |  |      \
    |  |       \
    0  1--2--3--4
    |
    +--0----R---1-->

*/

{
	double a, b, c;

  	a = 1.000 / 3.000;
  	b = 2.000 / 3.000;
  	c = 1.000;

  	t[0] =    4.500 * ( a - r - s ) * ( b - r - s ) * ( c - r - s );
  	t[1] =   13.500 * r * ( b - r - s ) * ( c - r - s ) ;
  	t[2] = - 13.500 * r * ( a - r ) * ( c - r - s );
  	t[3] =    4.500 * r * ( a - r ) * ( b - r );
  	t[4] =   13.500 * s * ( b - r - s ) * ( c - r - s );
 	t[5] =   27.000 * r * s * ( c - r - s );
  	t[6] =   13.500 * r * s * ( r - a );
  	t[7] =   13.500 * s * ( s - a ) * ( c - r - s );
  	t[8] =   13.500 * r * s * ( s - a );
  	t[9] =    4.500 * s * ( a - s ) * ( b - s );

  	dtdr[0] =    4.500 * ( ( a - s ) * ( 2.000 * r - c - b + 2.000 * s ) \
    	                   - ( s - b ) * ( s - c ) - 2.000 * ( 2.000 * s - b - c ) * r - 3.000 * r * r );
  	dtdr[1] =   13.500 * (( s - b ) * ( s - c ) + 2.000 * ( 2.000 * s - b - c ) * r + 3.000 * r * r );
  	dtdr[2] = - 13.500 * ( a * ( c - s ) + 2.000 * ( s - a - c ) * r + 3.000 * r * r );
  	dtdr[3] =    4.500 * ( a * b - 2.000 * ( a + b ) * r + 3.000 * r * r );
  	dtdr[4] =   13.500 * s * ( 2.000 * s - b - c + 2.000 * r );
  	dtdr[5] =   27.000 * s * ( c - s - 2.000 * r );
  	dtdr[6] =   13.500 * s * ( 2.000 * r - a );
  	dtdr[7] = - 13.500 * s * ( s - a );
  	dtdr[8] =   13.500 * s * ( s - a);
  	dtdr[9] =    0.000;

  	dtds[0] =    4.500 * ( ( a - r ) * ( 2.000 * s - c - b + 2.000 * r ) \
  	  	           - ( r - b ) * ( r - c ) - 2.000 * ( 2.000 * r - b - c ) * s - 3.000 * s * s );
  	dtds[1] =   13.500 * r * ( 2.000 * s + 2.000 * r - b - c );
  	dtds[2] =   13.500 * r * ( a - r );
  	dtds[3] =    0.000;
  	dtds[4] =   13.500 * ( ( r - b ) * ( r - c ) + 2.000 * ( 2.000 * r - b - c ) * s + 3.000 * s * s );
 	dtds[5] =   27.000 * r * ( c - r - 2.000 * s );
 	dtds[6] =   13.500 * r * ( r - a );
  	dtds[7] = - 13.500 * ( a * ( c - r ) + 2.000 * ( r - c - a ) * s + 3.000 * s * s );
  	dtds[8] =   13.500 * r * ( 2.000 * s - a);
  	dtds[9] =    4.500 * ( a * b - 2.000 * ( a + b ) * s + 3.000 * s * s );

}
